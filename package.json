{
  "type": "module",
  "name": "theme-tailwind",
  "version": "1.0.0",
  "description": "Tailwind theme for Oruga UI",
  "main": "index.js",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mouadTaoussi/theme-tailwind/issues"
  },
  "homepage": "https://github.com/mouadTaoussi/theme-tailwind#readme",
  "scripts": {
    "dev": "vite --force",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint . --fix --quiet",
    "lint:style": "stylelint **/*.{css,scss} --fix --ignore-path .gitignore",
    "update": "ncu -u",
    "dev1": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "build1": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mouadTaoussi/theme-tailwind.git"
  },
  "devDependencies": {
    "@oruga-ui/examples": "0.9.3",
    "@oruga-ui/oruga-next": "0.9.3",
    "@types/node": "^22.10.5",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.2.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "core-js": "3.39.0",
    "eslint": "^9.17.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "npm-check-updates": "^17.1.13",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "sass": "1.83.1",
    "stylelint": "^16.12.0",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^6.10.0",
    "tailwindcss": "^4.1.7",
    "terser": "^5.37.0",
    "typescript": "5.7.2",
    "vite": "^6.0.7",
    "vite-plugin-banner": "^0.8.0",
    "vite-plugin-dts": "^4.4.0",
    "vite-plugin-static-copy": "^2.2.0",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vue-tsc": "2.2.0"
  },
  "keywords": [
    "oruga",
    "tailwind",
    "theme-tailwind"
  ],
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.7"
  }
}
